{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "server:build": "tsc -p server/tsconfig.json",
    "server:build:watch": "tsc -p server/tsconfig.json --watch",
    "server:start": "NODE_ENV=development nodemon --watch dist/server --exec \"node dist/server/index.js\"",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:build:watch\" \"npm run server:start\"",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --config video=false'",
    "lint": "eslint . --fix",
    "analyze": "vite build --mode analyze",
    "dev": "vite",
    "format": "prettier --write src/",
    "preview": "vite preview",
    "ship": "npm run test && git push",
    "test": "vite build && npm run lint && vitest run && npm run test:e2e:docker",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e '",
    "test:e2e:live": "CYPRESS_BASE_URL=https://winter-feather-5776.fly.dev cypress run --config video=false",
    "test:docker": "node tests/docker/run-docker-tests.js",
    "test:e2e:docker": "node tests/docker/run-e2e-tests.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.445.0",
    "@eslint/js": "^9.25.1",
    "@vitest/eslint-plugin": "^1.1.43",
    "@vueuse/core": "^13.1.0",
    "chart.js": "^4.4.9",
    "express": "^4.18.2",
    "pinia": "^3.0.2",
    "vue": "^3.4.27",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.5.0",
    "vue-toastification": "^2.0.0-rc.5",
    "vue-virtual-scroller": "^2.0.0-beta.8"
  },
  "devDependencies": {
    "@dagger.io/dagger": "^0.18.4",
    "@eslint/eslintrc": "^3.3.1",
    "@rushstack/eslint-patch": "^1.11.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^8.2.2",
    "cypress": "14.3.2",
    "eslint": "^9.25.1",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-vue": "^10.0.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.11",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^3.1.2",
    "vue-eslint-parser": "^10.1.3",
    "testcontainers": "^10.2.1"
  },
  "type": "module"
}
