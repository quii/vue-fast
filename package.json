{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "server:build": "tsc -p server/tsconfig.json",
    "server:build:watch": "tsc -p server/tsconfig.json --watch",
    "server:start": "NODE_ENV=development nodemon --watch dist/server --exec \"node dist/server/index.js\"",
    "minio:ensure": "node scripts/ensure-minio.js",
    "redis:ensure": "node scripts/ensure-redis.js",
    "dev:all": "concurrently \"npm run minio:ensure\" \"npm run redis:ensure\" \"npm run dev\" \"npm run server:build:watch\" \"npm run server:start\"",
    "test:unit": "vitest --reporter=dot --run --silent=passed-only",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --config video=false'",
    "lint": "eslint . --fix",
    "analyze": "vite build --mode analyze",
    "dev": "vite",
    "format": "prettier --write src/",
    "preview": "vite preview",
    "ship": "npm run test && git push",
    "test": "vite build && npm run lint && npm run test:unit && npm run test:e2e:docker",
    "test:e2e:dev": "concurrently \"npm run minio:ensure\" \"npm run server:build:watch\" \"NODE_ENV=development start-server-and-test 'vite dev --port 4173 & sleep 2 && npm run server:start' http://localhost:4173 'cypress open --e2e'\"",
    "test:e2e:live": "CYPRESS_BASE_URL=https://winter-feather-5776.fly.dev cypress run --config --headless video=false",
    "test:docker": "node tests/docker/run-docker-tests.js",
    "test:e2e:docker": "node tests/docker/run-e2e-tests.js",
    "test:server": "vitest run --config server/vitest.config.ts",
    "test:server:watch": "vitest --config server/vitest.config.ts",
    "cleanup-test-data:local": "node scripts/cleanup-script.js",
    "cleanup-test-data:live": "node scripts/cleanup-script.js --live"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.873.0",
    "@eslint/js": "^9.33.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vueuse/core": "^13.7.0",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "ioredis": "^5.7.0",
    "node-fetch": "^3.3.2",
    "pinia": "^3.0.3",
    "qrcode": "^1.5.4",
    "redis": "^5.8.2",
    "vue": "^3.5.20",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.5.1",
    "vue-toastification": "^2.0.0-rc.5",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@dagger.io/dagger": "^0.18.16",
    "@eslint/eslintrc": "^3.3.1",
    "@rushstack/eslint-patch": "^1.12.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vue/cli-plugin-typescript": "~5.0.9",
    "@vue/cli-service": "^5.0.9",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.2.1",
    "cypress": "15.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-cypress": "^5.1.1",
    "eslint-plugin-vue": "^10.4.0",
    "jsdom": "^26.1.0",
    "minio": "^8.0.5",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.0.13",
    "supertest": "^7.1.4",
    "testcontainers": "^11.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0"
  },
  "type": "module"
}
