{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --config video=false'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e '",
    "test:e2e:live": "CYPRESS_BASE_URL=https://winter-feather-5776.fly.dev cypress run --config video=false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vite build && npm run lint && vitest run && npm run test:e2e"
  },
  "dependencies": {
    "@vueuse/core": "^12.5.0",
    "html2pdf.js": "^0.10.2",
    "pinia": "^2.3.1",
    "vue": "^3.4.27",
    "vue-router": "^4.5.0",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@dagger.io/dagger": "^0.15.2",
    "@rushstack/eslint-patch": "^1.10.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "14.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.6.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.4.2",
    "start-server-and-test": "^2.0.10",
    "vite": "^5.4.11",
    "vite-plugin-pwa": "^0.20.0",
    "vitest": "^3.0.4"
  }
}
