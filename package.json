{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --config video=false'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e '",
    "test:e2e:live": "CYPRESS_BASE_URL=https://winter-feather-5776.fly.dev cypress run --config video=false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vite build && npm run lint && vitest run && npm run test:e2e"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.26",
    "vue-router": "^4.3.2",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@dagger.io/dagger": "^0.11.2",
    "@rushstack/eslint-patch": "^1.10.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.5",
    "cypress": "13.8.1",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.1.1",
    "eslint-plugin-vue": "^9.25.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.3",
    "vite": "^4.5.3",
    "vite-plugin-pwa": "^0.20.0",
    "vitest": "^1.5.2"
  }
}
